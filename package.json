{
  "name": "ab-vue",
  "version": "0.0.1",
  "description": "ab ui",
  "keywords": [
    "vue3",
    "ab ui"
  ],
  "files": [
    "lib/**/*"
  ],
  "types": "./lib/index.d.ts",
  "main": "./lib/abu.umd.js",
  "module": "./lib/abu.es.js",
  "exports": {
    ".": {
      "import": "./lib/abu.es.js",
      "require": "./lib/abu.umd.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "build:lib": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build:npm": "vite build"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "path-to-regexp": "^6.2.0",
    "@vitejs/plugin-vue": "^2.3.0",
    "path": "^0.12.7",
    "prettier": "^2.6.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vue-tsc": "^0.29.8"
  },
  "license": "ISC",
  "author": "Ham"
}