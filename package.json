{
  "name": "ab-vue",
  "version": "0.0.33",
  "description": "",
  "main": "dist/cjs/entry.js",
  "browser": "dist/es/entry.esm.js",
  "module": "dist/es/entry.esm.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "unpkg": "dist/ab-vue.min.js",
  "jsdelivr": "dist/ab-vue.min.js",
  "files": [
    "dist/*",
    "es",
    "cjs",
    "src/**/*.vue"
  ],
  "exports": {
    ".": {
      "node": {
        "import": "./dist/cjs/entry.js",
        "require": "./dist/cjs/entry.js"
      },
      "import": "./dist/es/entry.esm.js",
      "require": "./dist/cjs/entry.js",
      "types": "./dist/types/src/entry.esm.d.ts"
    },
    "./es": "./dist/es/entry.esm.js",
    "./cjs": "./dist/cjs/entry.js",
    "./es/": "./es/",
    "./cjs/": "./cjs/",
    "./package.json": "./package.json"
  },
  "sideEffects": [
    "dist/es/lib-components/**/*.css",
    "dist/cjs/lib-components/**/*.css",
    "*.css",
    "*.less"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --port 1000 dev/serve.ts",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && cpx \"src/lib-components/**/*.{css,less}\" dist/es/lib-components  && cpx \"src/lib-components/**/*.{css,less}\" dist/cjs/lib-components",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/"
  },
  "dependencies": {
    "@vant/use": "^1.3.6",
    "@vue/runtime-core": "^3.2.31",
    "@vue/shared": "^3.2.31",
    "quicklink": "^2.2.0",
    "throttles": "^1.0.1",
    "vue": "^3.2.31"
  },
  "peerDependencies": {
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/runtime-core": "^3.2.31",
    "@vue/shared": "^3.2.31",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.3.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.9",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.13",
    "@vue/cli-service": "^4.5.13",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "less": "3.9.0",
    "less-loader": "5.0.0",
    "minimist": "^1.2.5",
    "postcss": "^8.2.10",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-multi-input": "^1.3.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.49.9",
    "sass-loader": "10.1.1",
    "stylelint": "^14.6.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.7.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vue-loader": "^17.0.0",
    "vue-loader-v16": "^16.0.0-beta.5.4",
    "vue-style-loader": "^4.1.3"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tuolib/ab-vue.git",
    "directory": "src"
  }
}
